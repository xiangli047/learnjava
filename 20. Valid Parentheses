public boolean isValid(String s) {
        if(s.length() <= 1) return false;
        
        Stack<Character> stack = new Stack<Character>();
        for(int i = 0;i<s.length();i++){
            char tmp = s.charAt(i);
            if(tmp == '(' || tmp == '['||tmp == '{'){
                stack.push(s.charAt(i));
            }else{
                if(stack.size() == 0) return false;
                char top = stack.pop();
                if(top == '('){
                    if(tmp != ')') return false;
                }else if(top == '['){
                    if(tmp != ']') return false;
                }else if(top == '{'){
                    if(tmp != '}') return false;
                }
            }
            
        }
        return stack.size() == 0;
        
    }
