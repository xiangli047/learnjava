ListNode curr = new ListNode(0);
        curr.next = head;
        int i = n-m;
        int k = m;
        while(k > 1){
            curr = curr.next;
            k--;
        }
        ListNode reve = curr.next;
        while(i > 0){
            i--;
            ListNode tmp = reve.next;
            reve.next = tmp.next;
            tmp.next = curr.next;
            curr.next = tmp;
        }
        if(m == 1) head = curr.next;
        
        return head;
